{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "strategy": "NostalgiaForInfinityX6",
    "add_config_files": [
        "../strategies/NostalgiaForInfinity/configs/trading_mode-spot.json",
        "./pairlist.bitget.json",
        "../strategies/NostalgiaForInfinity/configs/blacklist-bitget.json",
        "./telegram.json"
    ],
    "max_open_trades": 4,
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "USD",
    "dry_run": true,
    "dry_run_wallet": 1000,
    "cancel_open_orders_on_exit": false,

    "unfilledtimeout": {
        "entry": 10,
        "exit": 10,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "entry_pricing": {
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing": {
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 1
    },
    "exchange": {
        "name": "bitget",
        "key": "",
        "secret": "",
        "password": "",
        "ccxt_config": {},
        "ccxt_async_config": {}
    },
    "telegram": {
        "enabled": true,
        "token": "",
        "chat_id": ""
    },
    "api_server": {
        "enabled": true,
        "listen_ip_address": "0.0.0.0",
        "listen_port": 8080,
        "verbosity": "error",
        "enable_openapi": false,
        "jwt_secret_key": "",
        "ws_token": "",
        "CORS_origins": [],
        "username": "",
        "password": ""
    },
    "bot_name": "BitgetX6Spot",
    "initial_state": "running",
    "force_entry_enable": false,
    "internals": {
        "process_throttle_secs": 5
    },
    "webhook": {
        "enabled": true,
        "format": "json",
        "url": "",
        "allow_custom_messages": true,
        "strategy_msg": {
            "Event": "Strategy Message",
            "Message": "{msg}"
        },
        "entry": {
            "action": "entry",
            "trade_id": "{trade_id}",
            "pair": "{pair}",
            "limit": "limit {limit:8f}",
            "stake": "{stake_amount:8f}",
            "stake_currency": "{stake_currency}",
            "exchange": "{exchange}"
        },
        "entry_cancel": {
            "action": "entry_cancel",
            "trade_id": "{trade_id}",
            "pair": "{pair}",
            "limit": "{limit:8f}",
            "stake": "{stake_amount:8f}",
            "stake_currency": "{stake_currency}",
            "exchange": "{exchange}"
        },
            "entry_fill": {
            "action": "entry_fill",
            "trade_id": "{trade_id}",
            "pair": "{pair}",
            "open_rate": "{open_rate:8f}",
            "exchange": "{exchange}"
        },
        "exit": {
            "action": "exit",
            "trade_id": "{trade_id}",
            "pair": "{pair}",
            "limit": "{limit:8f}",
            "stake": "{stake_amount:8f}",
            "stake_currency": "{stake_currency}",
            "exchange": "{exchange}",
            "profit_amount": "{profit_amount:8f}",
            "profit_ratio": "{profit_ratio:2f}"
        },
        "exit_cancel": {
            "action": "exit_cancel",
            "trade_id": "{trade_id}",
            "pair": "{pair}",
            "limit": "{limit:8f}",
            "stake": "{stake_amount:8f}",
            "stake_currency": "{stake_currency}",
            "exchange": "{exchange}",
            "profit_amount": "{profit_amount:8f}",
            "profit_ratio": "{profit_ratio:2f}"
        },
        "exit_fill": {
            "action": "exit_fill",
            "pair": "{pair}",
            "exchange": "{exchange}",
            "close_rate": "{close_rate:8f}.",
            "open_date": "{open_date}",
            "close_date": "{close_date}",
            "trade_id": "{trade_id}",
            "direction": "{direction}",
            "bot_name": "BitgetX6Spot"
        },
        "status": {
            "action": "status",
            "status": "{status}"
        }
    }
}
